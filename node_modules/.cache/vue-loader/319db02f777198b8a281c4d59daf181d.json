n\n.nodejs {\n  width: 100%;\n  animation: nodejs 2s;\n}\n\n.expressjs {\n  width: 100%;\n  animation: expressjs 2s;\n}\n\n/* ICONS STYLING */\n/* .fab fa-html5 {\n  color: aqua;\n} */\n\n@keyframes html {\n  /*animate html skills*/\n  0% {\n    width: 0%;\n  }\n\n  100% {\n    width: 80%;\n  }\n}\n\n@keyframes css {\n  /*animate css skills*/\n  0% {\n    width: 0%;\n  }\n\n  100% {\n    width: 93%;\n  }\n}\n\n@keyframes javascript {\n  /*animate javascript skills*/\n  0% {\n    width: 0%;\n  }\n\n  100% {\n    width: 50%;\n  }\n}\n\n@keyframes javascript {\n  0% {\n    width: 0%;\n  }\n\n  100% {\n    width: 30%;\n  }\n}\n\n@keyframes vuejs {\n  0% {\n    width: 0%;\n  }\n\n  100% {\n    width: 30%;\n  }\n}\n\n@keyframes nodejs {\n  0% {\n    width: 0%;\n  }\n\n  100% {\n    width: 30%;\n  }\n}\n\n@keyframes express {\n  0% {\n    width: 0%;\n  }\n\n  100% {\n    width: 30%;\n  }\n}\n</style>\n"]}]}