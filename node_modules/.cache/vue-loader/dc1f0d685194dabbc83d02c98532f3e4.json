<div class=\"row justify-content-center\"></div>\n    <a\n      href=\"https://codepen.io/reese1997 /\"\n      class=\"fab fa-codepen\"\n      target=\"_blank\"\n      style=\"font-size: 70px; color: #222\"\n    ></a>\n    <a\n      href=\"https://github.com/Reese1997/\"\n      class=\"fab fa-github\"\n      style=\"font-size: 70px; color: #222\"\n    ></a>\n    <a\n      href=\"https://www.linkedin.com/in/reese-abrahams-19525415b/\"\n      class=\"fa fa-linkedin\"\n    ></a>\n  </div>\n</template>\n\n<script>\nexport default {};\nconst sentences = [\n  \"Reese\",\n  \"Front-End Developer\",\n  \"Software Developer\",\n  \"Creative and logical\",\n];\n\nfunction waitForMs(ms) {\n  return new Promise((resolve) => setTimeout(resolve, ms));\n}\n\nasync function typeSentence(sentence, element, delay = 100) {\n  const letters = sentence.split(\"\");\n  let i = 0;\n  while (i < letters.length) {\n    await waitForMs(delay);\n    document.querySelector(element).append(letters[i]);\n    i++;\n  }\n}\n\nasync function deleteSentence(element) {\n  const sentence = document.querySelector(element).innerHTML;\n  const letters = sentence.split(\"\");\n  while (letters.length > 0) {\n    await waitForMs(100);\n    letters.pop();\n    document.querySelector(element).innerHTML = letters.join(\"\");\n  }\n}\n\nasync function sentenceLoop(sentenceList, element) {\n  let i = 0;\n  while (true) {\n    await typeSentence(sentenceList[i], element);\n    await waitForMs(1500);\n    await deleteSentence(element);\n    await waitForMs(500);\n    i++;\n    if (i >= sentenceList.length) {\n      i = 0;\n    }\n  }\n}\n\nsentenceLoop(sentences, \"#text\");\n\n</script>\n\n<style>\n.background {\n  background-image: url(\"../assets/aboutr.jpeg\");\n  \n  background-size: 300px 300px;\n  height: 100vh;\n  color: rgb(8, 8, 8);\n  background-position: fixed;\n  background-size: cover;\n  background-repeat: none;\n}\n\nheader {\n  display: flex;\n}\n\n.landing-text,\n.landing-img {\n  width: 50%;\n}\n\n.landing-text {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  padding: 30px;\n}\n\n.landing-img {\n  max-height: 100vh;\n  object-fit: cover;\n}\n\n@media screen and (max-width: 500px) {\n  header {\n    flex-direction: column;\n  }\n  .landing-text,\n  .landing-img {\n    width: 100%;\n  }\n}\n\n.home {\n  padding: 90px;\n  border-radius: 10px;\n  margin: 10px auto;\n  max-width: 600px;\n  cursor: pointer;\n  color: rgb(14, 13, 13);\n}\nh1 {\n  padding-top: 28%;\n  padding-left: 80px;\n  text-align: center;\n  color: rgb(223, 223, 235);\n  font-size: 80px;\n}\nh2 {\n  text-align: center;\n  color: rgb(255, 247, 247);\n  font-size: 70px;\n}\n\n.home a {\n  text-decoration: none;\n}\n\n#text {\n  position: relative;\n}\n\n#text:after {\n  width: 3px;\n  height: 100%;\n  background: black;\n  position: absolute;\n  animation: blink 1s infinite;\n}\n\n@keyframes blink {\n  0%,\n  49% {\n    background: rgb(252, 248, 248);\n  }\n  50%,\n  100% {\n    background: rgb(255, 252, 252);\n  }\n}\n\n.fa-codepen:before {\n  padding-left: 880px;\n}\n\n.fa:hover {\n}\n</style>\n"]}]}